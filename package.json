    "ejs": "^3.1.3",
    "envvar": "^2.0.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "moment": "^2.29.4",
    "plaid": "^9.3.0",
    "puppeteer": "^3.1.0",
    "puppeteer-core": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-plaid-link": "^2.3.0-alpha.0",
    "requirejs": "^2.3.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
